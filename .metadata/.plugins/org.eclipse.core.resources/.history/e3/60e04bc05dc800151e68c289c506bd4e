public class GridFrame extends JFrame{

	private JPanel panel = new JPanel();
	private Container container;
	
	public GridFrame(){

		panel.setBackground(Color.white);

		container = getContentPane();
		container.add(panel, BorderLayout.CENTER);
	}
	

